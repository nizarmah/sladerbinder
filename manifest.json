{
    "manifest_version": 2,
    "name": "Slader Binder - Slader CheatSheet PDF Creator",
    "short_name": "Slader Binder",
    "version": "1.0.1",
    "icons": {
        "128": "icon.png"
    },
    "background": {
        "scripts": [ 
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click to Create PDF"
    },
    "web_accessible_resources": [
        "main.css"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}